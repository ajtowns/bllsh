
; simulate (bip340_verify P M SIG) manually

def (bip340check R S E P) (secp256k1_muladd (rc nil S) (rc P E) (rc R 1))
def (mkR SIG) (substr SIG 0 32)
def (mkS SIG) (substr SIG 32 64)
def (dup X) (cat X X)
def (taghash TAG) (dup (sha256 TAG))
def (mkE R P M) (sha256 (taghash "BIP0340/challenge") R P M)
def (mybip340x R S P M) (bip340check R S (mkE R P M) P)
def (mybip340_verify P M SIG) (mybip340x (mkR SIG) (mkS SIG) P M)

